{
  "name": "currency-converter-api",
  "version": "1.0.0",
  "description": "A small NodeJS RESTful api that queries the open souce ExchangeRatesAPI. The main purpose of this custom API is to store the conversion rates on a daily basis in case of the event  of the external API goind down.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samogorm/currency-converter-api.git"
  },
  "keywords": [
    "reactjs",
    "typescript",
    "nodejs",
    "exchangerates",
    "restfulapi"
  ],
  "author": "Samantha O'Gorman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samogorm/currency-converter-api/issues"
  },
  "homepage": "https://github.com/samogorm/currency-converter-api#readme",
  "devDependencies": {
    "@types/algoliasearch": "^3.30.17",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^12.0.10",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "mongoose": "^5.6.2",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.5",
    "algoliasearch": "^3.33.0",
    "cors": "^2.8.5"
  }
}
